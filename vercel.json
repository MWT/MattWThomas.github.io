{
  "trailingSlash": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=14400"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2629746"
        }
      ]
    },
    {
      "source": "/assets/pdfs/(.*)/(.*)\\.pdf",
      "headers": [
        {
          "key": "Link",
          "value": "<https://mattwthomas.com/$1/$2/>; rel=\"canonical\""
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/gh/:match*/",
      "destination": "https://mwt.github.io/:match*/index.html"
    },
    {
      "source": "/gh/:match*",
      "destination": "https://mwt.github.io/:match*"
    }
  ]
}